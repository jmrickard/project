            <PropertyTypeFacet TypeId="INT16" />
            <!-- Document URL -->
            <PropertyTypeFacet TypeId="DOC19" />
        </ItemTypeFacet>

        <!-- E-mail -->
        <ItemTypeFacet TypeId="EM1" Subfacets="ExcludeSpecific">
            <!-- Unique Reference -->
            <PropertyTypeFacet TypeId="EM2" />
