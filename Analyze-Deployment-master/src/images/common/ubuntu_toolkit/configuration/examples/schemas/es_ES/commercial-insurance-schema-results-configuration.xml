            <PropertyTypeFacet TypeId="VEH15" />
            <!-- Chassis Number -->
            <PropertyTypeFacet TypeId="VEH16" />
            <!-- Vehicle Identification Number -->
            <PropertyTypeFacet TypeId="VEH17" />
        </ItemTypeFacet>

        <!-- Claim -->
        <ItemTypeFacet TypeId="CLML1" Subfacets="ExcludeSpecific">
            <!-- Description -->
            <PropertyTypeFacet TypeId="CLML2" />
            <!-- Unique Reference -->
            <PropertyTypeFacet TypeId="CLML3" />
        </ItemTypeFacet>

        <!-- Communication -->
        <ItemTypeFacet TypeId="COML1" Subfacets="ExcludeSpecific">
            <!-- Description -->
            <PropertyTypeFacet TypeId="COML2" />
