            <PropertyTypeFacet TypeId="OTHL2" />
            <!-- Unique Reference -->
            <PropertyTypeFacet TypeId="OTHL3" />
        </ItemTypeFacet>

        <!-- Vehicle Link -->
        <ItemTypeFacet TypeId="VEHL1" Subfacets="ExcludeSpecific">
            <!-- Description -->
            <PropertyTypeFacet TypeId="VEHL2" />
            <!-- Unique Reference -->
            <PropertyTypeFacet TypeId="VEHL3" />
        </ItemTypeFacet>

        <!-- Subject Of -->
        <ItemTypeFacet TypeId="SUB1" Subfacets="ExcludeSpecific">
            <!-- Description -->
            <PropertyTypeFacet TypeId="PT2" />
            <!-- Unique Reference -->
            <PropertyTypeFacet TypeId="SUB2" />
        </ItemTypeFacet>
    </Facets>
</tns:ResultsConfiguration>