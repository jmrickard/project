            <PropertyTypeFacet TypeId="ORG5" />
        </ItemTypeFacet>

        <!-- Person -->
        <ItemTypeFacet TypeId="PER1" Subfacets="ExcludeSpecific">
            <!-- Unique Reference -->
            <PropertyTypeFacet TypeId="PER2" />
