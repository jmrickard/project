            <PropertyTypeFacet TypeId="CLA4" />
        </ItemTypeFacet>

        <!-- Credit Card -->
        <ItemTypeFacet TypeId="CRC1" Subfacets="ExcludeSpecific">
            <!-- Unique Reference -->
            <PropertyTypeFacet TypeId="CRC2" />
            <!-- Credit Card Number -->
            <PropertyTypeFacet TypeId="CRC3" />
