<?xml version="1.0" encoding="UTF-8"?>

<!-- Results configuration for Commercial_Insurance_Schema.xml -->
<tns:ResultsConfiguration xmlns:tns="http://www.i2group.com/Schemas/2016-04-25/ResultsConfiguration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.i2group.com/Schemas/2016-04-25/ResultsConfiguration ResultsConfiguration.xsd ">
    <Facets InlineLimit="5" ViewAllLimit="100">
        <!-- Address -->
        <ItemTypeFacet TypeId="ADD1" Subfacets="ExcludeSpecific">
            <!-- Unique Reference -->
            <PropertyTypeFacet TypeId="ADD2" />
            <!-- Mailbox -->
            <PropertyTypeFacet TypeId="ADD11" />
            <!-- Easting -->
            <PropertyTypeFacet TypeId="ADD12" />
            <!-- Northing -->
            <PropertyTypeFacet TypeId="ADD13" />
            <!-- Latitude -->
            <PropertyTypeFacet TypeId="ADD14" />
            <!-- Longitude -->
            <PropertyTypeFacet TypeId="ADD15" />
            <!-- Combined Coordinates -->
            <PropertyTypeFacet TypeId="ADD21" />
            <!-- Example of a Metadata Facet -->
            <!--  <MetadataFacet Criterion="FirstUploaded" /> -->
        </ItemTypeFacet>

        <!-- Account -->
        <ItemTypeFacet TypeId="ACC1" Subfacets="ExcludeSpecific">
            <!-- Unique Reference -->
            <PropertyTypeFacet TypeId="ACC2" />
            <!-- Account Number -->
            <PropertyTypeFacet TypeId="ACC3" />
        </ItemTypeFacet>

        <!-- Claim -->
        <ItemTypeFacet TypeId="CLA1" Subfacets="ExcludeSpecific">
            <!-- Unique Reference -->
            <PropertyTypeFacet TypeId="CLA2" />
            <!-- Claim -->
            <PropertyTypeFacet TypeId="CLA3" />
