        <!-- Telephone -->
        <ItemTypeFacet TypeId="TEL1" Subfacets="ExcludeSpecific">
            <!-- Unique Reference -->
            <PropertyTypeFacet TypeId="TEL2" />
            <!-- Main Number -->
            <PropertyTypeFacet TypeId="TEL6" />
        </ItemTypeFacet>

        <!-- Vehicle -->
        <ItemTypeFacet TypeId="VEH1" Subfacets="ExcludeSpecific">
